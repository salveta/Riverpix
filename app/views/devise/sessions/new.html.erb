<div class='centrar'>
    <h2 class='login'>Sign in</h2>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class='letter-login'><%= f.label :email %></div>
      <div><%= f.email_field :email, autofocus: true, :placeholder=>'Insert email' %></div>

      <div class='password'><%= f.label :password %></div>
      <div><%= f.password_field :password, autocomplete: "off", :placeholder=>'Insert password' %></div>

      <% if devise_mapping.rememberable? -%>
        <div class='remember'><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
      <% end -%>

      <div><%= f.submit "Sign in" %></div>
    <% end %>
  <%= render "devise/shared/links" %>
</div>






